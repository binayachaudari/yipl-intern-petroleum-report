{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "arrow-parens": [1, "as-needed"],
    "no-unused-vars": ["error"],
    "no-console": 0,
    "comma-dangle": 0,
    "object-curly-newline": 0,
    "semi": 0,
    "consistent-return": 0,
    "no-underscore-dangle": 1
  }
}
